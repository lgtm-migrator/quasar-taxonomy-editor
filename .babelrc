{
    "presets": [
        "bili/babel",
        "vue"
    ],
    "plugins": [
        "syntax-dynamic-import",
        [
            "@babel/plugin-proposal-decorators",
            {
                "legacy": true
            }
        ],
        [
            "@babel/plugin-proposal-class-properties",
            {
                "loose": true
            }
        ],
        [
            "@babel/plugin-transform-runtime",
            {
                "corejs": false,
                "helpers": true,
                "regenerator": false,
                "useESModules": true,
                "absoluteRuntime": false
            }
        ]
    ]
}
